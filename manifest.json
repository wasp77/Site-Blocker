{
  "name": "waspy-blocker",
  "description": "Block unproductive sites.",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["content-script.js"]
    }
  ],
  "permissions": []
}
